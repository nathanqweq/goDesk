{
  "manifest_version": 2.0,
  "id": "godesk",
  "name": "goDesk",
  "version": "1.1.0",
  "namespace": "GoDesk",
  "description": "goDesk module (config viewer/editor).",
  "assets": {
    "css": ["godesk.css"],
    "js": ["godesk.js"]
  },
  "actions": {
    "godesk.config.view": {
      "class": "ConfigView",
      "view": "godesk.configview"
    },
    "godesk.config.edit": {
      "class": "ConfigEdit",
      "view": "godesk.configedit"
    }
  }
}
